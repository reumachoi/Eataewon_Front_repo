<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   xmlns:tools="http://schemas.android.com/tools"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="413dp"
                                                   tools:context=".mypageUpdateFragment">


    <EditText
            android:id="@+id/mypage_email"
            android:imeOptions="actionDone"
            android:layout_width="400dp"
            android:layout_height="50dp"
            android:backgroundTint="@color/black"
            android:ems="10"
            android:gravity="center"
            android:inputType="textPersonName"
            android:textAlignment="center"
            android:fontFamily="@font/font"
            android:textColor="@color/black"
            app:layout_constraintStart_toStartOf="parent"
            tools:ignore="TouchTargetSizeCheck,SpeakableTextPresentCheck"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="52dp" app:layout_constraintHorizontal_bias="0.48"/>
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="소개"
            android:fontFamily="@font/font"
            android:textColor="@color/black"
            android:textSize="15sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toTopOf="@+id/mypage_profilmsg"
            app:layout_constraintStart_toStartOf="@+id/mypage_profilmsg"
            android:id="@+id/textView19" android:layout_marginBottom="4dp" android:layout_marginStart="4dp"/>
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="이메일"
            android:fontFamily="@font/font"
            android:textColor="@color/black"
            android:textSize="15sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toTopOf="@+id/mypage_email"
            app:layout_constraintStart_toStartOf="@+id/mypage_email"
            android:id="@+id/textView14" android:layout_marginBottom="8dp" android:layout_marginStart="4dp"/>
    <EditText
            android:id="@+id/mypage_nickname"
            android:imeOptions="actionDone"
            android:layout_width="400dp"
            android:layout_height="50dp"
            android:backgroundTint="@color/black"
            android:ems="10"
            android:gravity="center"
            android:inputType="textPersonName"
            android:textAlignment="center"
            android:fontFamily="@font/font"
            android:textColor="@color/black"
            app:layout_constraintEnd_toEndOf="@+id/mypage_email"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/mypage_email"
            app:layout_constraintTop_toBottomOf="@+id/mypage_email"
            tools:ignore="TouchTargetSizeCheck,SpeakableTextPresentCheck"
            android:layout_marginTop="44dp"/>
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="닉네임"
            android:fontFamily="@font/font"
            android:textColor="@color/black"
            android:textSize="15sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="@+id/mypage_nickname"
            app:layout_constraintBottom_toTopOf="@+id/mypage_nickname" android:layout_marginBottom="4dp"
            android:layout_marginStart="4dp"/>
    <Button
            android:id="@+id/mypageUserupdateBtn"
            android:layout_width="200dp"
            android:layout_height="50dp"
            android:text="수정"
            android:fontFamily="@font/font"
            android:textSize="20dp"
            android:textStyle="bold"
            android:backgroundTint="@color/black"
            app:layout_constraintEnd_toEndOf="@+id/mypage_profilmsg"
            app:layout_constraintTop_toBottomOf="@+id/mypage_profilmsg"
            android:layout_marginTop="32dp" app:layout_constraintStart_toStartOf="@+id/mypage_profilmsg"
            app:layout_constraintHorizontal_bias="0.52"/>
    <EditText
            android:id="@+id/mypage_profilmsg"
            android:imeOptions="actionDone"
            android:layout_width="400dp"
            android:layout_height="50dp"
            android:backgroundTint="@color/black"
            android:ems="10"
            android:fontFamily="@font/font"
            android:gravity="center"
            android:inputType="textPersonName"
            android:textAlignment="center"
            android:textColor="@color/black"
            app:layout_constraintEnd_toEndOf="@+id/mypage_nickname"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/mypage_nickname"
            app:layout_constraintTop_toBottomOf="@+id/mypage_nickname"
            tools:ignore="TouchTargetSizeCheck,SpeakableTextPresentCheck"
            android:layout_marginTop="52dp"/>

</androidx.constraintlayout.widget.ConstraintLayout>