<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".DeleteActivity">

    <TextView
            android:text="정보 삭제 "
            android:layout_width="251dp"
            android:layout_height="42dp" android:id="@+id/textView2"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.039" app:layout_constraintHorizontal_bias="0.429"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/deletId"
            app:layout_constraintTop_toBottomOf="@+id/textView2"
            android:layout_marginTop="36dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="224dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintVertical_bias="0.001"
            app:layout_constraintStart_toStartOf="@+id/textView2"/>
    <TextView
            android:text="이름"
            android:layout_width="67dp"
            android:layout_height="44dp" android:id="@+id/textView6"
            app:layout_constraintEnd_toStartOf="@+id/deletId"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintHorizontal_bias="0.557"
            app:layout_constraintVertical_bias="0.128"/>
    <Button
            android:text="삭제버튼"
            android:layout_width="181dp"
            android:layout_height="88dp" android:id="@+id/deletebtn"
            app:layout_constraintTop_toBottomOf="@+id/deletId"
            android:layout_marginTop="160dp" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.439" app:layout_constraintVertical_bias="0.0"/>
</androidx.constraintlayout.widget.ConstraintLayout>