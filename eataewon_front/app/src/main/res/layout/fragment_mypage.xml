<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
            xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:app="http://schemas.android.com/apk/res-auto"
            xmlns:tools="http://schemas.android.com/tools"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/white"
            android:orientation="vertical">

    <TextView
            android:layout_width="83dp"
            android:layout_height="20dp"
            android:text="My Page"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintHorizontal_bias="0.464"
            app:layout_constraintVertical_bias="0.049" android:id="@+id/Title"/>
    <TextView
            android:layout_width="91dp"
            android:layout_height="22dp"
            android:text="자기 소개"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintHorizontal_bias="0.189"
            android:id="@+id/title3"
            app:layout_constraintTop_toBottomOf="@+id/NickCh"
            android:layout_marginTop="44dp" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.0"/>
    <TextView
            android:layout_width="173dp"
            android:layout_height="24dp"
            android:text="닉네임"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintHorizontal_bias="0.255"
            android:id="@+id/title2"
            app:layout_constraintTop_toBottomOf="@+id/Imagebtn" android:layout_marginTop="36dp"
    />
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="프로필 사진"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintHorizontal_bias="0.165"
            android:id="@+id/textView"
            app:layout_constraintTop_toBottomOf="@+id/Title" android:layout_marginTop="32dp"/>
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="호감도 "
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintHorizontal_bias="0.585"
            android:id="@+id/textView2"
            app:layout_constraintTop_toBottomOf="@+id/Title"
            android:layout_marginTop="32dp"/>
    <Button
            android:text="사진 변경"
            android:layout_width="103dp"
            android:layout_height="42dp" android:id="@+id/Imagebtn"
            app:layout_constraintTop_toBottomOf="@+id/imageView2"
            android:layout_marginTop="20dp" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintHorizontal_bias="0.299"
    />
    <Button
            android:text="수정"
            android:layout_width="99dp"
            android:layout_height="55dp" android:id="@+id/correct"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="92dp"
            android:layout_marginBottom="108dp" app:layout_constraintStart_toEndOf="@+id/Cancel"
            app:layout_constraintHorizontal_bias="0.557" app:layout_constraintTop_toBottomOf="@+id/IntroCh"
            app:layout_constraintVertical_bias="0.896"/>
    <Button
            android:text="취소"
            android:layout_width="100dp"
            android:layout_height="51dp" android:id="@+id/Cancel"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginBottom="108dp"
            app:layout_constraintTop_toBottomOf="@+id/IntroCh" app:layout_constraintVertical_bias="0.897"
            android:layout_marginStart="92dp"/>
    <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" tools:srcCompat="@tools:sample/avatars" android:id="@+id/imageView2"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/editTextTextPersonName3"
            app:layout_constraintHorizontal_bias="0.444" app:layout_constraintTop_toBottomOf="@+id/textView"
            android:layout_marginTop="40dp"/>
    <LinearLayout
            android:orientation="vertical"
            android:layout_width="101dp"
            android:layout_height="42dp"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.985" app:layout_constraintBottom_toBottomOf="@+id/Title"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintVertical_bias="1.0">
    </LinearLayout>
    <EditText
            android:layout_width="236dp"
            android:layout_height="38dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/IntroCh"
            app:layout_constraintTop_toBottomOf="@+id/title3"
            android:layout_marginTop="44dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="272dp"
            app:layout_constraintStart_toEndOf="@+id/Cancel" app:layout_constraintHorizontal_bias="0.9"/>
    <EditText
            android:layout_width="237dp"
            android:layout_height="49dp"
            android:inputType="textPersonName"

            android:ems="10"
            android:id="@+id/NickCh"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.279" app:layout_constraintTop_toBottomOf="@+id/title2"
            android:layout_marginTop="28dp"/>
    <EditText
            android:layout_width="170dp"
            android:layout_height="110dp"
            android:inputType="textPersonName"
            android:text="Name"
            android:ems="10"
            android:id="@+id/editTextTextPersonName3"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="108dp"
            app:layout_constraintTop_toBottomOf="@+id/textView2" android:layout_marginTop="40dp"/>


</androidx.constraintlayout.widget.ConstraintLayout>