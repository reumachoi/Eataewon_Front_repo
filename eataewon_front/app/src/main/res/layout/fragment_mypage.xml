<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        android:theme="@style/Theme.Eattaewon_back_kimminki.NoActionBar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:title="My Page"
        app:titleTextColor="@color/black"
        tools:ignore="MissingConstraints">

    </androidx.appcompat.widget.Toolbar>

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:layout_marginBottom="20dp"
        android:text="프로필 사진"
        android:textColor="@color/black"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/mypage_profil_image"
        app:layout_constraintStart_toStartOf="@+id/mypage_profil_image" />

    <ImageView
        android:id="@+id/mypage_profil_image"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_marginStart="80dp"
        android:layout_marginTop="84dp"
        android:background="@drawable/style_profil_img"
        android:scaleType="centerCrop"
        android:src="@drawable/imgeview_sample"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbar" />

    <TextView
        android:id="@+id/mypage_profilpic_uri"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="@+id/mypage_profil_image"
        app:layout_constraintStart_toStartOf="@+id/mypage_profil_image"
        app:layout_constraintTop_toBottomOf="@+id/mypage_profil_image" />

    <Button
        android:id="@+id/mypageProfilpicBtn"
        android:layout_width="80dp"
        android:layout_height="40dp"
        android:text="사진변경"
        app:layout_constraintEnd_toEndOf="@+id/mypage_profil_image"
        app:layout_constraintHorizontal_bias="0.475"
        app:layout_constraintStart_toStartOf="@+id/mypage_profil_image"
        app:layout_constraintTop_toBottomOf="@+id/mypage_profilpic_uri"
        tools:ignore="TouchTargetSizeCheck" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="5dp"
        android:text="아이디"
        android:textColor="@color/black"
        android:textSize="15sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/mypage_id"
        app:layout_constraintStart_toStartOf="@+id/mypage_id" />

    <TextView
        android:id="@+id/mypage_id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:textColor="#000000"
        android:textSize="25sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/mypage_likepoint"
        app:layout_constraintTop_toTopOf="@+id/mypage_profil_image" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="5dp"
        android:text="호감도"
        android:textColor="@color/black"
        android:textSize="15sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/mypage_likepoint"
        app:layout_constraintStart_toStartOf="@+id/mypage_likepoint" />

    <TextView
        android:id="@+id/mypage_likepoint"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="110dp"
        android:text="TextView"
        android:textColor="#000000"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/mypageProfilpicBtn"
        app:layout_constraintStart_toEndOf="@+id/mypageProfilpicBtn" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="5dp"
        android:text="이름"
        android:textColor="@color/black"
        android:textSize="15sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/mypage_name"
        app:layout_constraintStart_toStartOf="@+id/mypage_name" />

    <TextView
        android:id="@+id/mypage_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="48dp"
        android:gravity="center"
        android:text="TextView"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="25sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/mypage_id"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/mypage_id"
        app:layout_constraintTop_toBottomOf="@+id/mypage_id" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="10dp"
        android:text="이메일"
        android:textColor="@color/black"
        android:textSize="15sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/mypage_email"
        app:layout_constraintStart_toStartOf="@+id/mypage_email" />

    <EditText
        android:id="@+id/mypage_email"
        android:layout_width="400dp"
        android:layout_height="50dp"
        android:layout_marginTop="80dp"
        android:backgroundTint="@color/black"
        android:ems="10"
        android:gravity="center"
        android:inputType="textPersonName"
        android:textAlignment="center"
        android:textColor="@color/black"
        app:layout_constraintStart_toStartOf="@+id/mypage_profil_image"
        app:layout_constraintTop_toBottomOf="@+id/mypage_likepoint"
        tools:ignore="TouchTargetSizeCheck,SpeakableTextPresentCheck" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="10dp"
        android:text="닉네임"
        android:textColor="@color/black"
        android:textSize="15sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/mypage_nickname"
        app:layout_constraintStart_toStartOf="@+id/mypage_nickname" />

    <EditText
        android:id="@+id/mypage_nickname"
        android:layout_width="400dp"
        android:layout_height="50dp"
        android:layout_marginTop="60dp"
        android:backgroundTint="@color/black"
        android:ems="10"
        android:gravity="center"
        android:inputType="textPersonName"
        android:textAlignment="center"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="@+id/mypage_email"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="@+id/mypage_email"
        app:layout_constraintTop_toBottomOf="@+id/mypage_email"
        tools:ignore="TouchTargetSizeCheck,SpeakableTextPresentCheck" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="10dp"
        android:text="소개"
        android:textColor="@color/black"
        android:textSize="15sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/mypage_profilmsg"
        app:layout_constraintStart_toStartOf="@+id/mypage_profilmsg" />

    <EditText
        android:id="@+id/mypage_profilmsg"
        android:layout_width="400dp"
        android:layout_height="50dp"
        android:layout_marginTop="60dp"
        android:backgroundTint="@color/black"
        android:ems="10"
        android:gravity="center"
        android:inputType="textPersonName"
        android:textAlignment="center"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="@+id/mypage_nickname"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="@+id/mypage_nickname"
        app:layout_constraintTop_toBottomOf="@+id/mypage_nickname"
        tools:ignore="TouchTargetSizeCheck,SpeakableTextPresentCheck" />

    <Button
        android:id="@+id/mypage_cancleBtn"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:text="취소"
        android:textSize="20dp"
        android:textStyle="bold"
        android:backgroundTint="#989898"
        app:layout_constraintBottom_toBottomOf="@+id/mypage_updateBtn"
        app:layout_constraintStart_toStartOf="@+id/mypage_profilmsg"
        app:layout_constraintTop_toTopOf="@+id/mypage_updateBtn" />

    <Button
        android:id="@+id/mypage_updateBtn"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:layout_marginTop="36dp"
        android:text="수정"
        android:textSize="20dp"
        android:textStyle="bold"
        android:backgroundTint="@color/black"
        app:layout_constraintEnd_toEndOf="@+id/mypage_profilmsg"
        app:layout_constraintTop_toBottomOf="@+id/mypage_profilmsg" />

</androidx.constraintlayout.widget.ConstraintLayout>