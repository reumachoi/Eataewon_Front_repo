    <ImageView
            android:id="@+id/imageView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="80dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.501"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/icon" />

    <TextView
            android:id="@+id/LoginTitle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="30dp"
            android:text="EAT태원 클래스"
            android:textColor="#FF9800"
            android:textSize="50sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="@+id/imageView"
            app:layout_constraintStart_toStartOf="@+id/imageView"
            app:layout_constraintTop_toBottomOf="@+id/imageView" />

    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="20dp"
            android:text="ID"
            android:textColor="@color/black"
            android:textSize="30sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/loginID"
            app:layout_constraintEnd_toStartOf="@+id/loginID"
            app:layout_constraintTop_toTopOf="@+id/loginID"
            app:layout_constraintVertical_bias="0.0" />

    <EditText
            android:id="@+id/loginID"
            android:layout_width="200dp"
            android:layout_height="50dp"
            android:layout_marginTop="36dp"
            android:background="@drawable/style_edittext"
            android:ems="10"
            android:gravity="center"
            android:inputType="textPersonName"
            android:text="아이디"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.497"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/LoginTitle"/>
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="20dp"
            android:text="PW"
            android:textColor="@color/black"
            android:textSize="30sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/loginPw"
            app:layout_constraintEnd_toStartOf="@+id/loginPw"
            app:layout_constraintTop_toTopOf="@+id/loginPw"
            app:layout_constraintVertical_bias="0.0" />

    <EditText
            android:id="@+id/loginPw"
            android:layout_width="200dp"
            android:layout_height="50dp"
            android:layout_marginTop="30dp"
            android:background="@drawable/style_edittext"
            android:ems="10"
            android:gravity="center"
            android:inputType="textPersonName"
            android:text="비밀번호"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/loginID" />

    <Button
            android:id="@+id/login_Btn"
            android:layout_width="290dp"
            android:layout_height="40dp"
            android:layout_marginTop="12dp"
            android:background="#FF9800"
            android:text="로그인"
            android:textSize="20sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/loginPw"
            tools:ignore="TextContrastCheck" app:layout_constraintHorizontal_bias="0.496"/>

    <Button
            android:id="@+id/button2"
            android:layout_width="74dp"
            android:layout_height="20dp"
            android:layout_marginEnd="48dp"
            android:background="#00ff0000"
            android:text="ID/PW 찾기"
            android:textColor="@color/black"
            android:textSize="15sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/signUpAtivity_Btn"
            app:layout_constraintEnd_toStartOf="@+id/signUpAtivity_Btn"
            app:layout_constraintVertical_bias="1.0"
            app:layout_constraintTop_toBottomOf="@+id/google_sign_in_button"/>
    <Button
            android:id="@+id/signUpAtivity_Btn"
            android:layout_width="78dp"
            android:layout_height="29dp"
            android:background="#00ff0000"
            android:text="회원가입"
            android:textColor="@color/black"
            android:textSize="15sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="@+id/loginPw"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/google_sign_in_button" android:layout_marginTop="24dp"/>
    <ImageButton
            android:id="@+id/kakao_login_Button"
            android:layout_width="290dp"
            android:layout_height="40dp"
            android:src="@drawable/kakao_login_medium_wide"
            android:background="@android:color/transparent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="104dp"
            app:layout_constraintHorizontal_bias="0.495"
            app:layout_constraintTop_toBottomOf="@+id/login_Btn"/>

    <com.google.android.gms.common.SignInButton

            android:id="@+id/google_sign_in_button"
            android:layout_width="290dp"
            android:layout_height="40dp"
            tools:ignore="MissingConstraints"
            app:layout_constraintTop_toBottomOf="@+id/kakao_login_Button"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginTop="4dp" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.076" app:layout_constraintHorizontal_bias="0.495"/>

</androidx.constraintlayout.widget.ConstraintLayout>
